{
  "plugins": [],
  "source": {
    "include": ["assets/js"],
    "includePattern": ".js$",
    "excludePattern": "(node_modules/|documentation)"
  },
  "tags": {
    "allowUnknownTags": true,
    "dictionaries": ["jsdoc","closure"]
  },
  "templates": {
    "cleverLinks": false,
    "monospaceLinks": false
  },
  "opts": {
    "destination": "./documentation/",
    "recurse": true
  }
}